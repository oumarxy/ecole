                                slideBy: 4,
                                autoplay: 0,
                                autoplayHoverPause: 0,
                                autoplayTimeout: 0,
                                autoplaySpeed: 1000,
                                startPosition: 0,
                                mouseDrag: 1,
                                touchDrag: 1,
                                autoWidth: false,
                                responsive: {
